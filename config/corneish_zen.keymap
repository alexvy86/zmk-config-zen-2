#define THUMB2 &lt LY_MIS1 TAB
#define THUMB3 &lt LY_NAV SPACE
#define THUMB4 &mo_sk LY_NUM LSHFT
#define THUMB5 &lt LY_MIS2 BSPC
#define THUMB6 &kp ENTER

#define NAV_LM4 &mt LSHFT PRV_VD
#define NAV_LM3 &mt LALT NXT_VD

/ {
    combos {
        compatible = "zmk,combos";

        // Caps word on main thumb keys
        combo_caps_word {
            timeout-ms = <50>;
            key-positions = <LH0 RH0>;
            bindings = <&caps_word>;
        };
    };
};

